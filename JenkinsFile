@Library('testUtils')_
pipeline 
{
    agent any
    stages
    {
        stage('Demo') 
        {
            steps
            {
                echo 'Hello world'
                sayHello 'Paco'
                script 
                {
                    def changeLogSets = currentBuild.changeSets
                    for (int i = 0; i < changeLogSets.size(); i++) 
                    {
                        def entries = changeLogSets[i].items
                        for (int j = 0; j < entries.length; j++) 
                        {
                            def entry = entries[j]
echo "Domain is"
                            echo entry.getMsg()
                        }
                    }
                
                }
            }
        }
    }
}
