@Library('testUtils')_

stage('Demo') {
    git credentialsId: '8e36fdb6-0e42-4c75-abee-905d99f8884c', url: 'https://github.com/ssanchez227/hello-world/'

    echo 'Hello world'
    sayHello 'Paco'
    script {
      def changeLogSets = currentBuild.changeSets
        for (int i = 0; i < changeLogSets.size(); i++) {
            def entries = changeLogSets[i].items
            for (int j = 0; j < entries.length; j++) {
                def entry = entries[j]
                echo entry.getDomain()
            }
        }
        
    }
}

